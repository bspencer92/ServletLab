    <servlet-class>com.servlet.GpaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GpaServlet</servlet-name>
    <url-pattern>/GpaServlet</url-pattern>
  </servlet-mapping>
</web-app>